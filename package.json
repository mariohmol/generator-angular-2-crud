    "yosay": "^2.0.2"
  },
  "devDependencies": {},
  "eslintConfig": {
    "extends": [
      "xo-space/esnext",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "templates"
    ]
  },
  "license": "MIT"
